model operators "Operators" {
   types {
      t1 "Type 1" {
          v1 "Val1"
          v2 "Val2"
      }
   }
   product bar "bar" {
      param baz "baz" Int
      param p2 "Param 2" t1
      
      constraints {
         constraint on baz "fooconstraint" !(5 + -10 > 0)
         constraint "c3" true && false || (true && false)
         constraint "c31" !(!(20 - (-20) == 40))
         constraint "c5" contains(value(p2), "foo") == true
         constraint "c6" !(50 <= 50)
         constraint "c7" (20 >= 30) && false
      }
   }
}