			"constraints": [
				{
					"label": "Electric engines must use electric fuel",
					"param": "fuel",
					"expr": "(!(callFun(\"value\", getEnumParam(\"engine\")) == getValue(\"electrical\")) || (callFun(\"value\", getEnumParam(\"fuel\")) == getValue(\"electricity\")))"
				},
				{
					"label": "Ellert and Tesla only produces electric cars",
					"param": "engine",
					"expr": "(!((callFun(\"value\", getEnumParam(\"brand\")) == getValue(\"ellert\")) || (callFun(\"value\", getEnumParam(\"brand\")) == getValue(\"tesla\"))) || (callFun(\"value\", getEnumParam(\"engine\")) == getValue(\"electrical\")))"
				},
				{
					"label": "Are you an alcoholic?",
					"param": "alcohol",
					"expr": "(callFun(\"value\", getPrimParam(\"alcohol\", \"DoubleTy\")) < 0.5)"
				}
			]
