			],
			"constraints": [
				{
					"label": "Ram must be positive",
					"param": "ram",
					"expr": "(callFun(\"value\", getPrimParam(\"ram\", \"IntTy\")) > 0.0)"
				},
				{
					"label": "32-bits can at most have 4g ram",
					"param": "ram",
					"expr": "((callFun(\"value\", getEnumParam(\"arch\")) == getValue(\"x86\")) ? ((callFun(\"value\", getPrimParam(\"ram\", \"IntTy\")) <= 4.0)) : (true))"
				},
				{
					"label": "You must select at least one GPU",
					"param": null,
					"expr": "!((callFun(\"value\", getEnumParam(\"gpu1\")) == getValue(\"nogpu\")) && (callFun(\"value\", getEnumParam(\"gpu2\")) == getValue(\"nogpu\")))"
				},
				{
					"label": "Only Gigabyte motherboards support SLI",
					"param": "gpu2",
					"expr": "(!!(callFun(\"value\", getEnumParam(\"mb\")) == getValue(\"gigabytemb\")) || (callFun(\"value\", getEnumParam(\"gpu2\")) == getValue(\"nogpu\")))"
				},
				{
					"label": "At least 2Ghz",
					"param": "ghz",
					"expr": "(callFun(\"value\", getPrimParam(\"ghz\", \"DoubleTy\")) > 2.0)"
				}
			]
		},
